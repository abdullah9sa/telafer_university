 "css": "@font-face {\r\n  font-family: 'Tajawal';\r\n  src: url('/assets/telafer_university/fonts/Tajawal-Regular.ttf') format('truetype');\r\n  font-weight: normal;\r\n  font-style: normal;\r\n}\r\n\r\n@font-face {\r\n  font-family: 'Tajawalbold';\r\n  src: url('/assets/telafer_university/fonts/Tajawal-Bold.ttf') format('truetype');\r\n  font-weight: bold;\r\n  font-style: bold;\r\n}\r\n\r\nh2 {\r\n  font-family: 'Tajawalbold', sans-serif;\r\n\r\n}\r\ndiv, tr, td {\r\n  font-family: 'Tajawal', sans-serif;\r\n  \r\n}\r\n\r\n\r\n[lang=\"ar\"], .rtl {\r\n  font-family: 'Tajawal', sans-serif;\r\n}\r\n",
